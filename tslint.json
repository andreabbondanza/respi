{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "one-line":false,
        "quotemark":false,
        "trailing-comma":false,
        "variable-name":false,
        "no-trailing-whitespace":true,
        "whitespace":false,
        "no-unused-expression": false,
        "object-literal-sort-keys":false,
        "curly":false,
        "no-console": false,
        "no-unnecessary-initializer":false,
        "semicolon":false
    },
    "rulesDirectory": []
}